<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.min.css">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"></script>

    @RenderSection("scripts", required:false)

</head>

<body>

    <nav class="navbar navbar-expand-sm bg-dark">
        <div class="container-fluid">
            <ul class="navbar-nav">

                <li class="nav-item  mx-2 my-auto">
                    <a class="navbar-brand" href="#">
                        <img class="rounded-circle" style="width:80px" src="data:image/png;base64," />
                    </a>
                </li>

                <li class="nav-item my-auto">
                    <h4 class="nav-link text-white mx-4" style="font-size:28px;">MVC Surf Shop</h4>
                </li>

                <li class="nav-item my-auto">
                    @Html.ActionLink("Home", "Index", "Home", null, new { @class = "nav-link text-white mx-2" })
                </li>

                <li class="nav-item my-auto mx-2" style="color: grey; font-size:x-large; margin-top: 10px;">&vert;</li>

                
                <div class="my-auto dropdown">

                    <button type="button" class="btn" data-bs-toggle="dropdown">

                        @Html.ActionLink("Shop", "Index", "Home", null, new
                        {
                            @class = "nav-link dropdown-toggle text-white mx-2",
                            id = "navbarDropdown",
                        style = "background-color: transparent",
                            role = "button",
                            data_toggle = "dropdown",
                        })

                    </button>

                    <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <li>@Html.ActionLink("Customer", "ViewOnly", "Products", null, new { @class = "nav-link dropdown-item" })</li>
                        <li>@Html.ActionLink("Admin", "Index", "Products", null, new { @class = "nav-link dropdown-item" })</li>
                    </ul>

                </div>
                

                <li class="nav-item my-auto mx-2" style="color: grey; font-size:x-large; margin-top: 10px;">&vert;</li>

                <li class="nav-item my-auto mx-2">
                    @Html.ActionLink("Rent", "Index", "Rentals", null, new { @class = "nav-link text-white mx-2" })
                </li>

                <li class="nav-item my-auto mx-2" style="color: grey; font-size:x-large; margin-top: 10px;">&vert;</li>

                <li class="nav-item my-auto mx-2">
                    @Html.ActionLink("Surf", "Surf", "Surf", null, new { @class = "nav-link text-white mx-2" })
                </li>

                <li class="nav-item my-auto mx-2" style="color: grey; font-size:x-large; margin-top: 10px;">&vert;</li>

                <li class="nav-item my-auto mx-2">
                    @Html.ActionLink("Contact", "Contact", "Home", null, new { @class = "nav-link text-white mx-2" })
                </li>
            </ul>
        </div>
    </nav>

    <hr>

    <div class="container body-content">

        @RenderBody()

        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Rad Chad Industries</p>
        </footer>
    </div>
</body>

</html>
